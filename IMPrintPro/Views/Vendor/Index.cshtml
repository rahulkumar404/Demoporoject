
@{
    ViewData["Title"] = "Index";
}


<div class="container" id="filedesign" >
    
    <form method="post" class="vendor-form" asp-action="ImportExcelFile" asp-controller="Excel" enctype="multipart/form-data" style="padding:1px;">
        <p>Vendor Page</p>
        <div class="form-group">
            <label> File</label>
            <input type="file" class="fileId" name="FormFile" accept=".xlsx" />
        </div>
        <div class="form-group">
            <input type="submit" class="btn btn-sm btn-primary" value="Upload" />
            <a href="#">Download Sample</a>
        </div>
    </form>
</div>
